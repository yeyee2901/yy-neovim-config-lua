snippet insert_picture "Insert a picture"
\begin{figure}[h]
    \centering
    \includegraphics[scale=$1]{$2}
    \caption{$3}
    \label{gambar:$4}
\end{figure}
endsnippet

snippet verb "Inline code listing"
\verb|$1|
endsnippet


snippet code_listing "Without file"
\begin{lstlisting}[language=$1 , caption={$2}]

\end{lstlisting}
endsnippet


snippet code_source "Source from file"
\lstinputlisting[language=$1 , caption={$2}]{$3}
endsnippet


